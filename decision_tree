digraph g {
	node [height=.1 shape=record]
	outlook [label=outlook]
	humidity [label=humidity]
	outlook -> humidity [label=Sunny]
	humidity [label=humidity]
	HighNo [label=No]
	humidity -> HighNo [label=High]
	NormalYes [label=Yes]
	humidity -> NormalYes [label=Normal]
	OvercastYes [label=Yes]
	outlook -> OvercastYes [label=Overcast]
	wind [label=wind]
	outlook -> wind [label=Rain]
	wind [label=wind]
	WeakYes [label=Yes]
	wind -> WeakYes [label=Weak]
	StrongNo [label=No]
	wind -> StrongNo [label=Strong]
}
